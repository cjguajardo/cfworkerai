{
  "name": "cfworkerai",
  "description": "Vercel AI SDK provider for Cloudflare Workers",
  "version": "0.0.2",
  "license": "Apache-2.0",
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "dev": "tsup --watch",
    "lint": "eslint \"./**/*.ts*\"",
    "type-check": "tsc --noEmit",
    "prettier-check": "prettier --check \"./**/*.ts*\"",
    "test": "pnpm test:node && pnpm test:edge",
    "test:edge": "vitest --config vitest.edge.config.js --run",
    "test:node": "vitest --config vitest.node.config.js --run"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "dependencies": {
    "@ai-sdk/provider": "0.0.12",
    "@ai-sdk/provider-utils": "1.0.2"
  },
  "devDependencies": {
    "@types/node": "^18",
    "dotenv": "^16.4.5",
    "tsup": "^8",
    "typescript": "5.1.3",
    "vercel": "^34.3.1",
    "vitest": "^2.0.2"
  },
  "peerDependencies": {
    "ai": "^3.2.22",
    "zod": "^3.23.8"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://sdk.vercel.ai/docs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cjguajardo/cfworkerai.git"
  },
  "bugs": {
    "url": "https://github.com/cjguajardo/cfworkerai/issues"
  },
  "keywords": [
    "ai"
  ]
}